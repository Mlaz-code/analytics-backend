# =============================================================================
# PIKKIT CREDENTIALS TEMPLATE
# =============================================================================
# Copy this file to .env and fill in your actual credentials
# =============================================================================

# -----------------------------------------------------------------------------
# Supabase Configuration
# -----------------------------------------------------------------------------
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# -----------------------------------------------------------------------------
# Netlify Configuration
# -----------------------------------------------------------------------------
NETLIFY_TOKEN=your-netlify-token-here
NETLIFY_SITE_ID=your-site-id-here
NETLIFY_DASHBOARD_URL=https://your-dashboard.netlify.app

# -----------------------------------------------------------------------------
# Telegram Bot Configuration
# -----------------------------------------------------------------------------
TELEGRAM_BOT_TOKEN=your-bot-token-here
TELEGRAM_CHAT_ID=your-chat-id-here

# -----------------------------------------------------------------------------
# n8n Webhook Configuration
# -----------------------------------------------------------------------------
N8N_WEBHOOK_URL=https://your-n8n.com/webhook/your-webhook
N8N_WORKFLOW_ID=your-workflow-id
N8N_BASE_URL=https://your-n8n.com

# -----------------------------------------------------------------------------
# Pikkit API Configuration
# -----------------------------------------------------------------------------
PIKKIT_API_BASE=https://prod-website.pikkit.app
PIKKIT_API_TOKEN=your-pikkit-api-token-here

# -----------------------------------------------------------------------------
# ML API Configuration
# -----------------------------------------------------------------------------
ML_API_URL=http://localhost:5000

# -----------------------------------------------------------------------------
# Backend API Configuration
# -----------------------------------------------------------------------------
EXTENSION_API_URL=http://localhost:8000

# -----------------------------------------------------------------------------
# Dashboard Configuration
# -----------------------------------------------------------------------------
DASHBOARD_URL=https://your-dashboard.com
